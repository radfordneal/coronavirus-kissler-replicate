#!/bin/bash

export LANG=en_CA.UTF-8

if [ x$R == x ]; then R=R; fi

for proxy in proxyW proxyA proxyB proxyC proxyD \
             proxyWo proxyAo proxyBo proxyCo proxyDo \
             proxyWss proxyAss proxyBss proxyCss proxyDss \
             proxyWm proxyAm proxyBm proxyCm proxyDm \
             proxyWn proxyAn proxyBn proxyCn proxyDn \
#            proxyWs proxyAs proxyBs proxyCs proxyDs
do
  for method in fmm # natural hyman
  do
    $R --save --no-restore \
       --args $proxy $method <R-est.R >&R-est-$proxy-$method.out
    # cp .RData R-est-$proxy-$method.RData
    $R --save --no-restore \
       --args $proxy $method filter <R-est.R >&R-est-$proxy-$method-filter.out
    # cp .RData R-est-$proxy-$method-filter.RData
  done
done
